pkgname=unzipko
pkgver=1.2
pkgrel=1
pkgdesc="Unzip perl script for Korean CP949 Zip Archive"
arch=('any')
url="http://arch.or.kr"
license=('GPL')
depends=('perl-archive-zip')
source=(unzipko)
md5sums=('cf7d39cfd96fcdafc0241dd79d81d7f0')
build() {
install -d ${pkgdir}/usr/bin
install -Dm 755 unzipko ${pkgdir}/usr/bin/
}
